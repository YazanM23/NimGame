            <Button fx:id="Easy" mnemonicParsing="false" onAction="#easyMode" prefHeight="26.0" prefWidth="70.0" text="Easy" />
            <Button fx:id="Medium" mnemonicParsing="false" onAction="#mediumMode" prefWidth="70.0" text="Medium" />
