      <TextField fx:id="playerName" layoutX="451.0" layoutY="476.0" prefHeight="28.0" prefWidth="112.0" promptText="Enter your Name" style="-fx-background-color: transperant; -fx-border-color: white; -fx-border-width: 0px 0px 2px 0px;" />
   </children>
