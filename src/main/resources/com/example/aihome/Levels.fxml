<AnchorPane prefHeight="573.0" prefWidth="1020.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aihome.LevelsController">
   <children>
      <ImageView fitHeight="573.0" fitWidth="1020.0" layoutX="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Images/apps.54764.9007199266387342.3183a9e3-fd2f-4afc-88d4-387cce2e5a75.2301d39f-737a-49d6-ae56-23408d006da7.jpeg" />
